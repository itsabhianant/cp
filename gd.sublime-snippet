	<content><![CDATA[
void getDivisors(ll n) {
    for(ll i = 1;i*i<=n;i++) {
	if(n%i==0) {
	    cout<<i<<' ';
	    if(i!=n/i)
		cout<<n/i<<' ';
        }
    }
}
]]></content>
