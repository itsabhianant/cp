	<content><![CDATA[
void getDivisors(ll n) {
    ll i;
    for(i=1;i*i<n;i++) {
        if(!(n%i))
            cout<<i<<' ';
    }
    for(;i>=1;i--) {
        if(!(n%i))
            cout<<n/i<<' ';
    }
}
]]></content>
